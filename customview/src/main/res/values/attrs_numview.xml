<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--时间轴-->
    <declare-styleable name="numview">
        <!--矩形边长-->
        <attr name="nvRectLength" format="integer"/>
        <!--文字大小-->
        <attr name="nvTextSize" format="integer"/>
        <!--矩形中加减号与边框距离-->
        <attr name="nvIntervalLength" format="integer"/>
        <!--默认文字个数-->
        <attr name="nvTextLength" format="integer"/>
        <!--显示的文字-->
        <attr name="nvNum" format="integer"/>
        <!--方框圆角-->
        <attr name="nvRound" format="integer"/>
        <!--方框颜色颜色-->
        <attr name="nvRectColor" format="color"/>
        <!--字体颜色颜色-->
        <attr name="nvTextColor" format="color"/>
        <!--每次改变的大小-->
        <attr name="nvChangeNum" format="integer"/>
        <!--最小值-->
        <attr name="nvMinNum" format="integer"/>
        <!--最大值-->
        <attr name="nvMaxNum" format="integer"/>
        <!--是否打开动画-->
        <attr name="nvIsStartAnima" format="boolean"/>


    </declare-styleable>
</resources>